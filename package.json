{
  "name": "nextdealbackend",
  "version": "1.0.13",
  "description": "this is a nextdeal backend",
  "main": "index.ts",
  "type": "module",
  "private": false,
  "scripts": {
    "dev": "nodemon --config nodemon.json",
    "dev:debug": "nodemon --config nodemon.json --inspect",
    "dev1": "ts-node src/index.ts",
    "build-local": "node esbuild.config.js",
    "build-prod": "npm i && rimraf dist && node esbuild.config.js",
    "build": "npm i && rimraf dist && node esbuild.config.js",
    "build1": "npm i && rimraf dist && tsup",
    "start": "node dist/index.cjs",
    "start:prod": "NODE_ENV=production node dist/index.cjs",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "clean": "rimraf dist coverage",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "format": "prettier --config .prettierrc --write .",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "test:cov": "vitest run --coverage",
    "release": "semantic-release",
    "swagger-autogen": "node swagger.js",
    "docker:dev": "docker-compose up",
    "docker:dev:build": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:build": "docker-compose -f docker-compose.prod.yml up -d --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup-python": "python3 -m venv venv && ./venv/bin/pip install -r requirements.txt",
    "import-locations": "tsx importLocation.ts",
    "import-from-sheets": "./venv/bin/python import_locations_from_sheets.py",
    "import-locations-full": "./venv/bin/python import_locations_from_sheets.py && tsx importLocation.ts",
    "load-test": "node testing/load-test.js",
    "load-test:artillery": "npx artillery run testing/load-test-config.yml",
    "load-test:quick": "node testing/load-test.js --quick",
    "load-test:heavy": "node testing/load-test.js --heavy",
    "bandwidth-test": "node testing/bandwidth-test.js",
    "bandwidth-test:artillery": "npx artillery run testing/bandwidth-test-config.yml",
    "bandwidth-test:quick": "node testing/bandwidth-test.js --quick",
    "bandwidth-test:heavy": "node testing/bandwidth-test.js --heavy",
    "unit-test": "node testing/unit-tests.js",
    "integration-test": "node testing/integration-tests.js",
    "api-test": "node testing/api-tests.js",
    "test:all": "npm run unit-test && npm run integration-test && npm run api-test",
    "test:quick": "npm run unit-test",
    "test:full": "npm run load-test && npm run bandwidth-test && npm run test:all",
    "test:rate-limits": "node testing/test-rate-limits.js",
    "cleanup-duplicates": "node scripts/cleanup-duplicates.js"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-comprehend": "^3.0.0",
    "@aws-sdk/client-rekognition": "^3.844.0",
