    "@aws-sdk/s3-request-presigner": "^3.600.0",
    "@swc/core": "^1.7.26",
    "@types/compression": "^1.7.5",
    "@types/nodemailer": "^6.4.17",
    "@types/sharp": "^0.32.0",
    "@types/twilio": "^3.19.3",
    "aws-sdk": "^2.1652.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.7.7",
    "bad-words": "^4.0.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parser": "^3.2.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "envalid": "^8.0.0",
    "esbuild": "^0.25.10",
    "express": "^4.21.2",
    "express-rate-limit": "^7.2.0",
    "firebase-admin": "^13.4.0",
    "helmet": "^7.2.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.11.4",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.2",
    "natural": "^8.0.1",
    "nodemailer": "^6.10.1",
    "nodemon": "^3.1.4",
    "pino-http": "^10.0.0",
    "razorpay": "^2.9.4",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tesseract.js": "^6.0.1",
    "ts-node": "^10.9.2",
    "twilio": "^5.5.2",
    "typeorm": "^0.3.25",
    "uuid": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.3",
    "@types/bad-words": "^3.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "artillery": "^2.0.23",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "lint-staged": "^15.2.2",
    "mock-aws-s3": "^4.0.2",
    "nock": "^14.0.5",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "semantic-release": "^24.0.0",
    "supertest": "^7.0.0",
    "swagger-autogen": "^2.23.7",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "5.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^3.2.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "lint-staged": {
    "**/*": "npm run format",
    "**/*.{js,ts}": "npm run lint:fix"
  },
